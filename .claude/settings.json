{
  "includeCoAuthoredBy": false,
  "language": "japanese",
  "permissions": {
    "ask": [
      "Bash(git push:*)",
      "Bash(cargo install:*)",
      "Bash(cargo add:*)",
      "Bash(npm install:*)"
    ],
    "deny": [
      "Read(.env*)",
      "Read(*.key)",
      "Read(*.pem)",
      "Bash(sudo:*)",
      "Bash(chmod:*)",
      "Bash(chown:*)",
      "Bash(rm:*)",
      "Bash(find:* -delete:*)",
      "Bash(find:* -exec:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git merge:*)",
      "Bash(git push:*--force:*)",
      "Bash(git push:*-f:*)",
      "Bash(git rebase:*)",
      "Bash(git reset:*)",
      "Bash(git rm:*)",
      "Bash(cargo publish:*)",
      "Bash(npm publish:*)"
    ]
  }
}
